# Template file for configure script

CSRCS = @CSRCS@ 
OD = @OD@
MODDIR = @MODDIR@
MODULE_NAME = @MODULE_NAME@
TARGET = @TARGET@
CFLAGS = @CFLAGS@
EXTRA_CFLAGS = @EXTRA_CFLAGS@
LDFLAGS = @LDFLAGS@
ROOT_DIR = @ROOT_DIR@

ifeq ($(DEBUG),1)
	CFLAGS += -g -DDEBUG
endif

COBJS = $(patsubst %.c,$(OD)/%.o,$(CSRCS))

all: @ALL_DEPS@

$(OD):
	@OD_RULE@ 

$(COBJS): $(OD)/%.o : %.c 
	@COBJS_RULE@

$(TARGET): @TARGET_DEPS@
	@TARGET_RULE@

__temp_link:
	@TEMP_RULE@

modpost:
	@MODPOST_RULE@
	@MODPOST_CC_RULE@
	
clean:
	@CLEAN_RULE@

